{
  "name": "get-user-os",
  "version": "1.0.0",
  "description": "get-user-os is a lightweight, easy-to-use JavaScript library that provides a simple API to detect the operating system of a user. It's built with TypeScript and works seamlessly across different browsers.",
  "main": "./build/src/index.js",
  "type": "module",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc && swc src -d build --config-file .swcrc",
    "publish": "npm publish",
    "co": "sui-mono commit",
    "lint": "eslint ."
  },
  "keywords": [
    "Operating System Detection",
    "User Agent",
    "JavaScript Library",
    "TypeScript",
    "Cross-Browser Compatibility"
  ],
  "author": {
    "name": "Raul Catalinas Esteban",
    "email": "raulcatalinas@gmail.com",
    "url": "https://github.com/RaulCatalinas"
  },
  "license": "MIT",
  "devDependencies": {
    "@s-ui/mono": "2.45.0",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.26",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-love": "71.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^7.1.0",
    "markdownlint": "0.35.0",
    "prettier": "3.3.3",
    "typescript": "5.6.2"
  },
  "files": [
    "build/**/*"
  ],
  "exports": {
    "./types": "./build/types"
  },
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaulCatalinas/GetUserOS"
  },
  "bugs": {
    "url": "https://github.com/RaulCatalinas/GetUserOS/issues",
    "email": "raulcatalinas@gmail.com"
  }
}
